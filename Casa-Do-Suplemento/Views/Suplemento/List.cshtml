@model IEnumerable<Casa_Do_Suplemento.Models.Suplemento>

<div>
    <h2>Todos os Suplementos</h2>
</div>

@foreach(var suplemento in Model)
{
    <div >
        <h4>@suplemento.Nome</h4>
        <div class = "ImgSuplementos">
            <p><img src ="@suplemento.ImagemUrl"></p>
        </div>
        <h3>Preço : @suplemento.Preco.ToString("c")</h3>
    </div>
}
